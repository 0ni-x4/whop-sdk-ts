{
  "name": "@whop-sdk/core",
  "version": "0.0.1",
  "description": "",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "types": "./dist/index.d.ts"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rimraf src dist && pnpm run gen && pnpm run transpile && pnpm run update-package-version",
    "gen": "openapi --input https://data.whop.com/api/v2/swagger_doc --output ./src --useOptions --indent 2 --name WhopSDK --request ./request/request.ts",
    "transpile": "concurrently \"pnpm run transpile:esm\" \"pnpm run transpile:cjs\" \"pnpm run transpile:types\" && pnpm run transpile:fix",
    "transpile:esm": "swc src/**/*.ts src/*.ts -d dist/esm --config-file .swc/esm.swcrc",
    "transpile:cjs": "swc src/**/*.ts src/*.ts -d dist/cjs --config-file .swc/cjs.swcrc",
    "transpile:types": "tsc --emitDeclarationOnly",
    "transpile:fix": "node scripts/fix-output.mjs",
    "update-package-version": "node scripts/update-package-version.mjs"
  },
  "engines": {
    "node": ""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc-node/register": "^1.5.4",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.20",
    "@types/fs-extra": "^9.0.13",
    "concurrently": "^7.6.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "openapi-typescript-codegen": "^0.23.0",
    "rimraf": "^3.0.2",
    "tsconfig": "workspace:^0.0.0",
    "typescript": "^4.9.3"
  }
}
