{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "@whop-sdk/core#build": {
      "dependsOn": ["repo-utils#build"],
      "outputs": ["dist/**"],
      "cache": false,
      "inputs": ["src/**"]
    },
    "repo-utils#build": {
      "outputs": ["dist/**"],
      "cache": true,
      "inputs": ["src/**"]
    },
    "dev": {
      "cache": false,
      "dependsOn": ["@whop-sdk/core#build"]
    }
  }
}
